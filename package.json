{
  "name": "@kube-design/components",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "main": "./cjs/index.js",
  "unpkg": "./umd/@kube-design/components.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "typings": "./es/index.d.ts",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "gulp",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,scss}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "pub": "npm publish ./lib",
    "pub:alpha": "npm publish ./lib --tag alpha",
    "pub:dev": "npm publish ./lib --tag dev",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@pitrix/lego-locale": "^0.1.11",
    "@statoscope/webpack-plugin": "^5.24.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@umijs/lint": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "classnames": "^2.3.2",
    "css-loader": "^6.7.2",
    "del": "^6.1.1",
    "dumi": "^1.1.49",
    "eslint": "^8.23.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^4.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "through2": "^4.0.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-stream": "^7.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "GravityCDN": "./umd/@kube-design/components.js",
  "authors": []
}
